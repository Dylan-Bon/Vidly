@model List<Vidly.Models.Movie>
@{
	ViewBag.Title = "Movies";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Movie List</h2>
<table class="table table-hover">
	<thead>
		<tr><th>Movie Name</th></tr>
	</thead>
	<tbody>
		@foreach (var movie in Model) {
			<tr><td>@movie.Name</td></tr>
		}
	</tbody>
</table>